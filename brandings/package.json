{
  "name": "threefold",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "build:prod": "ionic-app-scripts build --prod --nobrowser --enableLint false && node tools/finish_production_build.js",
    "build:zip": "npm run build:prod && npm run build:zip-only",
    "build:zip-only": "rm -f threefold.zip && mv www/index.html www/cordova.html && cd www && zip -r ../threefold.zip . && cd .. && mv www/cordova.html www/index.html",
    "build:embedded-app": "npm run build:prod && rm -f threefold.zip && cd www && zip -r ../threefold.zip .",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "start": "npm run ionic:serve",
    "sort_translations": "node tools/sort_i18n.js"
  },
  "dependencies": {
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/compiler-cli": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@ionic-native/browser-tab": "^4.5.3",
    "@ionic-native/core": "4.5.3",
    "@ionic-native/in-app-browser": "^4.5.3",
    "@ionic-native/splash-screen": "4.5.3",
    "@ionic-native/status-bar": "4.5.3",
    "@ionic/storage": "2.1.3",
    "@ngrx/effects": "^5.1.0",
    "@ngrx/store": "^5.1.0",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "angular2-chartjs": "^0.4.1",
    "angular2-qrcode": "^2.0.1",
    "chart.js": "^2.7.1",
    "cordova-plugin-console": "^1.1.0",
    "cordova-plugin-device": "^2.0.1",
    "cordova-plugin-inappbrowser": "^2.0.2",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "ionic-angular": "^3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "marked": "^0.3.17",
    "rogerthat-plugin": "git+https://github.com/rogerthat-platform/cordova-rogerthat-plugin.git#master",
    "rxjs": "5.5.6",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.8",
    "@types/chartjs": "0.0.31",
    "@types/marked": "^0.3.0",
    "@types/node": "^9.4.6",
    "codelyzer": "^4.1.0",
    "ionic": "3.19.1",
    "typescript": "2.7.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-console": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-inappbrowser": {}
    },
    "platforms": []
  }
